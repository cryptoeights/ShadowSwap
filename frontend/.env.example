# ShadowSwap Environment Configuration
# Copy this file to .env.local and fill in your values

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Contract Addresses (Arbitrum Sepolia)
NEXT_PUBLIC_SHADOW_POOL_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_SHADOW_HOOK_ADDRESS=0x0000000000000000000000000000000000000000

# Mock Token Addresses (for testing ETH/WETH with real price tracking)
# These are deployed on Arbitrum Sepolia
NEXT_PUBLIC_MOCK_ETH_ADDRESS=0xA270162c70f7107F0960139DE0bC13E9d870FD3E
NEXT_PUBLIC_MOCK_WETH_ADDRESS=0x3eD186BF2807866179De0D6EEFd1D62Efe2AbD5e

# iExec Configuration
NEXT_PUBLIC_IEXEC_IAPP_ADDRESS=0x834255dF01eE89d5096371a7eeFaF4332d4e2bfF

# Subgraph URL (The Graph hosted service or Studio)
NEXT_PUBLIC_SUBGRAPH_URL=

# Optional: RPC URLs (uses public defaults if not set)
NEXT_PUBLIC_ARBITRUM_SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc
NEXT_PUBLIC_ARBITRUM_RPC=https://arb1.arbitrum.io/rpc
